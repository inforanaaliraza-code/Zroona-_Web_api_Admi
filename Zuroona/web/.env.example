# Copy to .env.local and fill values. Do not commit .env.local.
# For production (Vercel/Netlify/etc.), set these in the host's Environment Variables.

# API Configuration (local dev)
NEXT_PUBLIC_API_BASE_URL=http://localhost:3434/api/
NEXT_PUBLIC_API_URL=http://localhost:3434/api

# AWS S3 Configuration
NEXT_PUBLIC_S3_BUCKET=
NEXT_PUBLIC_S3_REGION=
NEXT_PUBLIC_S3_URL=
NEXT_PUBLIC_S3_DIR=

# Google Maps API (required for map & address autocomplete)
# Get key: https://console.cloud.google.com/apis/credentials
# Restrict to: HTTP referrers (localhost + https://zuroona.sa/*). See GOOGLE_MAPS_SETUP.md
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=

# Payment Gateway (Moyasar)
NEXT_PUBLIC_MOYASAR_KEY=

# Node Environment
NODE_ENV=development

# App download (store links for /download-app page)
NEXT_PUBLIC_GOOGLE_PLAY_APP_URL=
NEXT_PUBLIC_APP_STORE_APP_URL=
